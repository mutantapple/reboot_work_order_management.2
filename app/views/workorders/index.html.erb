<h1>Listing workorders</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>Work status</th>
      <th>Customer</th>
      <th>Store</th>
      <th>Check in by</th>
      <th>Test by</th>
      <th>Finish by</th>
      <th>Take posession after</th>
      <th>Paid diaganostic fee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workorders.each do |workorder| %>
      <tr>
        <td><%= workorder.store %></td>
        <td><%= workorder.work_status %></td>
        <td><%= workorder.customer_id %></td>
        <td><%= workorder.store_id %></td>
        <td><%= workorder.check_in_by %></td>
        <td><%= workorder.test_by %></td>
        <td><%= workorder.finish_by %></td>
        <td><%= workorder.take_posession_after %></td>
        <td><%= workorder.paid_diaganostic_fee %></td>
        <td><%= link_to 'Show', workorder %></td>
        <td><%= link_to 'Edit', edit_workorder_path(workorder) %></td>
        <td><%= link_to 'Destroy', workorder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workorder', new_workorder_path %>
