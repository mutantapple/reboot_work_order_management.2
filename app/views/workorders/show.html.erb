<%- model_class = Workorder -%>
<div class="page-header">
  <h1><%=t '.title', :default => Workorder %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:work_status) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.work_status %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:customer_id) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.customer_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:store_id) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.store_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:check_in_by) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.check_in_by %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:test_by) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.test_by %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:finish_by) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.finish_by %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:take_posession_after) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.take_posession_after %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:paid_diaganostic_fee) %></dt>
      <dd><pre class="prettyprint"><%= @workorder.paid_diaganostic_fee %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              workorders_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_workorder_path(@workorder), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              workorder_path(@workorder),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

